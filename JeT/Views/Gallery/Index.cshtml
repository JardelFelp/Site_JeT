@using JeT.Models

@model List<Photo>

@{
    ViewBag.Title = "Index";
}

@if (Model.Count() > 0)
{
    foreach (Photo photo in Model)
    {
        <div class="col-md-3 portfolio-item text-center">
            <a class="d-block mb-4 h-100">
                <img class="img-fluid img-thumbnail" src="/content/img/gallery/@(photo.FilePath)" alt="@(photo.FileName)" >
            </a>
        </div>
    }
}
else
{
    <h3 class="text-center">Sem imagens enviadas :(</h3>
}
